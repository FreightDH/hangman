{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "root": true,
  "plugins": ["prettier", "import"],
  "extends": [
    "plugin:prettier/recommended",
    "prettier"
  ],
  "rules": {
    "import/no-mutable-exports": "off",
    "no-mutable-exports": "off",
    "no-param-reassign": "off",
    "no-multi-assign": "off",
    "no-plusplus": "off",
    "import/extensions": "off",
  }
}
